<%- include('partials/header');-%>
<h1>COMPOSE</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="FormControlInput1">Title</label>
        <input type="text" class="form-control" id="FormControlInput1" placeholder="Enter the title" name="postTitle" autocomplete="off">
      </div>
    <div class="form-group">
      <label for="FormControlTextarea1">Post</label>
      <textarea class="form-control" id="FormControlTextarea1" rows="7" name="postBody" autocomplete="off"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
  </form>
  
<%- include('partials/footer')-%> 
